import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Arrays;

public class longesthappyface {
    public static void main(String[] args) {
        String str="ababab";
        int[] arr=new int[str.length()];

        String [] arrpr = new String[str.length()-1];
        String [] arrsu = new String[str.length()-1];
        ArrayList <String> newarr = new ArrayList<>();
        for(int i =0;i<str.length()-1;i++){
            arrpr[i]= str.substring(0, i+1);
            arrsu[i]= str.substring(str.length()-i-1,str.length());
        }
        for(int i =0;i<arrpr.length;i++){
            if(arrpr[i].equals(arrsu[i])){
                newarr.add(arrpr[i]);
            }
        }
        System.out.println(newarr);

    String longestString = newarr.get(0);
    for (String element : newarr) {
        if (element.length() > longestString.length()) {
            longestString = element;
        }
    }
    System.out.println(longestString);

        


    }
}
